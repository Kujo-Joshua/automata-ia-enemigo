      <Button mnemonicParsing="false" onAction="#onSinNovedad" text="Sin novedad" />
      <Button mnemonicParsing="false" onAction="#onContactoVisual" text="Contacto visual" />
      <Button mnemonicParsing="false" onAction="#onObjetivoEnMira" text="Objetivo en mira" />
      <Button mnemonicParsing="false" onAction="#onRastroPerdido" text="Rastro Perdido" />
      <Button mnemonicParsing="false" onAction="#onCoberturaVisualizada" text="Cobertura visualizada" />
      <Button mnemonicParsing="false" onAction="#onRecibirAtaque" text="Recibir ataque" />
      <Button mnemonicParsing="false" onAction="#onEnemigoCambioPasillo" text="Enemigo cambio de pasillo" />
