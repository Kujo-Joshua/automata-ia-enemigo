<VBox alignment="CENTER" prefHeight="638.0" prefWidth="828.0" spacing="20.0" stylesheets="@stylees.css" xmlns="http://javafx.com/javafx/25" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.hotline.PrimaryController">
